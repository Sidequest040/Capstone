{
    "manifest_version": 3,
    "name": "Cache Clearing Tool",
    "version": "1.0",
    "permissions": [
      "browsingData"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "Clear Cache Tool"
    }
  }
  